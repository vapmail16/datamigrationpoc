{
  "name": "Customer Profile",
  "description": "Target schema for customer profile data",
  "version": "1.0",
  "fields": [
    {
      "name": "customer_id",
      "data_type": "string",
      "required": true,
      "description": "Unique identifier for the customer",
      "default_value": null
    },
    {
      "name": "first_name",
      "data_type": "string",
      "required": true,
      "description": "Customer's first name",
      "default_value": null
    },
    {
      "name": "last_name",
      "data_type": "string",
      "required": true,
      "description": "Customer's last name",
      "default_value": null
    },
    {
      "name": "email",
      "data_type": "string",
      "required": true,
      "description": "Customer's email address",
      "default_value": null
    },
    {
      "name": "phone",
      "data_type": "string",
      "required": false,
      "description": "Customer's phone number",
      "default_value": null
    },
    {
      "name": "address",
      "data_type": "object",
      "required": false,
      "description": "Customer's address information",
      "default_value": null
    },
    {
      "name": "created_at",
      "data_type": "date",
      "required": true,
      "description": "Date when the customer record was created",
      "default_value": "current_timestamp"
    },
    {
      "name": "age",
      "data_type": "number",
      "required": false,
      "description": "Customer's age in years",
      "default_value": null
    },
    {
      "name": "is_active",
      "data_type": "boolean",
      "required": true,
      "description": "Whether the customer account is active",
      "default_value": "true"
    },
    {
      "name": "preferences",
      "data_type": "array",
      "required": false,
      "description": "List of customer preferences and interests",
      "default_value": null
    },
    {
      "name": "subscription_tier",
      "data_type": "string",
      "required": true,
      "description": "Customer's subscription level (basic, premium, enterprise)",
      "default_value": "basic"
    },
    {
      "name": "last_login",
      "data_type": "date",
      "required": false,
      "description": "Timestamp of customer's last login",
      "default_value": null
    },
    {
      "name": "account_balance",
      "data_type": "number",
      "required": true,
      "description": "Current account balance in USD",
      "default_value": "0.00"
    },
    {
      "name": "payment_methods",
      "data_type": "array",
      "required": false,
      "description": "List of saved payment methods",
      "default_value": null
    },
    {
      "name": "notes",
      "data_type": "string",
      "required": false,
      "description": "Additional notes or comments about the customer",
      "default_value": null
    }
  ]
}